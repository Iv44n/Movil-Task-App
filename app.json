{
  "expo": {
    "name": "kron",
    "slug": "kron",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "kron",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.kron"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#181818"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.app.kron"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser",
      "expo-localization",
      [
        "expo-splash-screen", 
        {
         "backgroundColor": "#181818",
          "image": "./assets/icon.png",
          "dark": {
            "image": "./assets/icon.png",
            "backgroundColor": "#181818"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "089cf7eb-58aa-4ff8-862b-75fb2235c5e9"
      }
    },
    "owner": "ivan.ia"
  }
}
